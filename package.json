{
  "name": "table-tennis-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext js,ts --fix src && tsc --noEmit",
    "test": "jest",
    "generate": "graphql-codegen",
    "start": "node -r ./register.js src/index.js",
    "watch": "tsc --watch"
  },
  "lint-staged": {
    "src/{**,}/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-typescript": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "ajv": "^6.11.0",
    "apollo-server": "^2.14.2",
    "babel-plugin-transform-numeric-separator": "^7.0.0-beta.3",
    "chalk": "^3.0.0",
    "core-js": "^3.6.4",
    "faker": "^4.1.0",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.0-beta.1",
    "graphql-scalars": "^1.0.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "rimraf": "^3.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.12.2",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-resolvers": "^1.12.2",
    "@types/faker": "^4.1.9",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-eslint": "^10.0.3",
    "babel-register-ts": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.0.7",
    "typescript": "^3.7.5"
  }
}
